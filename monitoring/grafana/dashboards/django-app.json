{
  "uid": "tochka-django-app",
  "title": "Tochka Django App",
  "tags": ["django", "prometheus", "python", "tochka"],
  "timezone": "",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "editable": true,
  "graphTooltip": 0,
  "fiscalYearStartMonth": 0,
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
    "time_options": ["5m", "15m", "30m", "1h", "3h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "type": "query",
        "name": "job",
        "label": "job",
        "datasource": { "type": "prometheus", "uid": "Prometheus" },
        "refresh": 1,
        "query": { "query": "label_values(up, job)", "refId": "Prometheus-job" },
        "includeAll": false,
        "multi": false,
        "current": { "selected": false, "text": "django-app", "value": "django-app" },
        "options": []
      },
      {
        "type": "query",
        "name": "instance",
        "label": "instance",
        "datasource": { "type": "prometheus", "uid": "Prometheus" },
        "refresh": 1,
        "query": { "query": "label_values(up{job=~\"$job\"}, instance)", "refId": "Prometheus-instance" },
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": ["All"], "value": ["$__all"] },
        "options": []
      },
      {
        "type": "query",
        "name": "method",
        "label": "method",
        "datasource": { "type": "prometheus", "uid": "Prometheus" },
        "refresh": 1,
        "query": {
          "query": "label_values(django_http_requests_total_by_method_total{job=~\"$job\",instance=~\"$instance\"}, method)",
          "refId": "Prometheus-method"
        },
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": ["All"], "value": ["$__all"] },
        "options": []
      },
      {
        "type": "query",
        "name": "view",
        "label": "view",
        "datasource": { "type": "prometheus", "uid": "Prometheus" },
        "refresh": 1,
        "query": {
          "query": "label_values(django_http_requests_total_by_view_transport_method_total{job=~\"$job\",instance=~\"$instance\"}, view)",
          "refId": "Prometheus-view"
        },
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": ["All"], "value": ["$__all"] },
        "options": []
      },
      {
        "type": "interval",
        "name": "rate_interval",
        "label": "rate interval",
        "query": "30s,1m,2m,5m,10m",
        "current": { "selected": true, "text": "5m", "value": "5m" }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "App health",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Target UP",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 5, "w": 4, "x": 0, "y": 1 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max(up{job=~\"$job\",instance=~\"$instance\"})",
          "range": true,
          "editorMode": "code"
        }
      ]
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Process uptime (seconds)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 5, "w": 6, "x": 4, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "time() - max(process_start_time_seconds{job=~\"$job\",instance=~\"$instance\"})",
          "range": true,
          "editorMode": "code"
        }
      ]
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Open FDs",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
