      "targets": [
        {
          "refId": "A",
          "expr": "max(process_max_fds{job=~\"$job\",instance=~\"$instance\"})",
          "range": true,
          "editorMode": "code"
        }
      ]
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Python version",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "fieldConfig": { "defaults": { "mappings": [], "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] } }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "max by(version) (python_info{job=~\"$job\",instance=~\"$instance\"})",
          "range": true,
          "editorMode": "code",
          "legendFormat": "{{version}}"
        }
      ]
    },

